{
  "name": "psychology-love-test",
  "version": "1.0.0",
  "description": "Психологические тесты с красивым интерфейсом и админ-панелью",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio --port 5555",
    "prisma:migrate": "prisma migrate dev",
    "import-tests": "tsx scripts/import-tests.ts",
    "db:seed": "tsx scripts/seed-test.ts",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d --build",
    "docker:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "docker:prod:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "docker:prod:ps": "docker-compose -f docker-compose.prod.yml ps",
    "docker:build": "docker build -t psychology-love-test .",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "deploy:export-db": "tsx scripts/export-production-db.ts",
    "clean": "rm -rf .next node_modules/.cache",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@lottiefiles/react-lottie-player": "^3.6.0",
    "@prisma/client": "^6.17.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/howler": "^2.2.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/qrcode": "^1.5.5",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.0.0",
    "framer-motion": "^12.23.24",
    "gsap": "^3.13.0",
    "howler": "^2.2.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.263.1",
    "next": "15.5.6",
    "prisma": "^6.17.1",
    "qrcode": "^1.5.4",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "15.5.6",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "ts-jest": "^29.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "psychology",
    "love",
    "test",
    "nextjs",
    "react",
    "typescript",
    "docker"
  ],
  "author": "moralfager",
